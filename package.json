    "leaflet": "^1.9.4",
    "lucide-react": "^0.462.0",
    "luxon": "^3.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^16.2.4",
